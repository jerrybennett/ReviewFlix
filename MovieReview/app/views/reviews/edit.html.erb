<h1>Edit Review for: <%= @movie.title %></h1>
<%= form_for([@movie, @review]) do |f| %>
  <div id="rating-form">
    <label>Rating</label>
  </div><br />
  <%= f.label :comment %>:<br />
  <%= f.text_area :comment, class: "form-control" %><br />
  <%= f.submit %>
<% end %>

<script>
  $('#rating-form').raty({
    path: '/assets/',
    scoreName: 'review[rating]'
  });
</script>